            <column name="INITIATED_BY" type="VARCHAR2(255)"/>
            <column name="AUTHORIZED_BY" type="VARCHAR2(255)"/>
            <column name="PAYMENT_REF" type="VARCHAR2(255)"/>
            <column name="CUSTOMER_ACCT_NUMBER" type="VARCHAR2(255)"/>
            <column name="STATUS" type="VARCHAR2(255)"/>
